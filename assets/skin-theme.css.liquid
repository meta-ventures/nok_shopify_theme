/*=====================================
  blog post css
======================================*/
.blog-post {
  position: relative;
}

/* blog image css */
.blog-post {
  border-radius: 5px;
  overflow: hidden;
}
.blog-post .blog-main-img{
  position: relative;
  overflow: hidden;
}
.blog-post .blog-main-img a.blog-img{
  position: relative;
}
.blog-post .blog-main-img a.blog-img::before {
  background-image: linear-gradient(180deg, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.7));
  content: '';
  position: absolute;
  bottom: 0px;
  left: 0px;
  right: 0px;
  width: 100%;
  height: 100%;
}

/* blog content css */
.blog-post .blog-post-content{
  padding: 30px;
  position: absolute;
  right: 0;
  bottom: 0px;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  text-align: center;
  -webkit-transition: all .3s ease-in-out 0s;
  -o-transition: all .3s ease-in-out 0s;
  transition: all .3s ease-in-out 0s;
}
.blog-post .blog-post-content .blog-tag-date span.date-time{
  color: #ffffff;
  font-size: 16px;
  text-transform: uppercase;
  font-weight: 600;
}
.blog-post .blog-post-content .blog-tag-date span.date-time span{
  {% if settings.enable_rtl %}
  margin-right: 3px;
  {% else %}
  margin-left: 3px;
  {% endif %}
}
.blog-post .blog-post-content .blog-tag-date span.date-time span:first-child{
  {% if settings.enable_rtl %}
  margin-right: 0px;
  {% else %}
  margin-left: 0px;
  {% endif %}
}
.blog-post .blog-post-content h2{
    color: #ffffff;
    font-size: 20px;
    font-weight: 500;
    margin-top: 11px;
}
.blog-post .blog-post-content h2 a{
  color: #ffffff;
  display: block;
}
.blog-post .blog-post-content h2 a:hover{
  opacity: 0.7;
}
.blog-post .blog-post-content p{
  display: none;
  color: #ffffff;
  font-size: 20px;
  margin-top: 5px;
  line-height: 35px;
}
.blog-post .blog-post-content .blog-author{
  display: none;
  margin-top: 4px;
}
.blog-post .blog-post-content .blog-author .author{
  color: var(--font-color-body);
  font-size: 16px;
  font-weight: 600;
}
.blog-post .blog-post-content a.read-btn{
  margin-top: 16px;
}
@media(max-width: 1199px){
  .blog-post .blog-post-content .blog-tag-date span.date-time{
    font-size: 14px;
  }
  .blog-post .blog-post-content p{
    font-size: 16px;
    line-height: 30px;
  }
}
@media(max-width: 767px){
  .blog-post .blog-post-content h2{
    font-size: 16px;
  }
  .blog-post .blog-post-content{
    padding: 15px;
  }
  .blog-post .blog-post-content p{
    font-size: 14px;
  }
}

/*=====================================
  pagination css
======================================*/
/* page-area css */
.paginatoin-area {
  padding-top: {{ settings.section_padding_top }}px;
}
@media (max-width: 1199px) {
  .paginatoin-area {
    padding-top: {{ settings.section_padding_top_ip }}px;
  }
}
@media (max-width: 767px) {
  .paginatoin-area {
    padding-top: {{ settings.section_padding_top_xs }}px;
  }
}

/* page-number css */
.paginatoin-area ul.pagination-box {
  display: flex;
  align-items: center;
  justify-content: center;
}
.paginatoin-area ul.pagination-box li {
  {% if settings.enable_rtl %}
  margin-left: 5px;
  {% else %}
  margin-right: 5px;
  {% endif %}
  line-height: 0;
}
.paginatoin-area ul.pagination-box li:last-child {
  {% if settings.enable_rtl %}
  margin-left: 0px;
  {% else %}
  margin-right: 0px;
  {% endif %}
}
.paginatoin-area ul.pagination-box li.number a {
  color: var(--font-color-body);
  font-size: 14px;
  height: 30px;
  width: 30px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #F9F9F9;
  font-weight: 600;
  line-height: 0;
  border-radius: 5px;
}
.paginatoin-area ul.pagination-box li.number:hover a,
.paginatoin-area ul.pagination-box li.number.active a {
  color: #ffffff;
  background-image: var(--gradient-color);
  -webkit-transition: all 0s ease-in-out 0s;
  -o-transition: all 0s ease-in-out 0s;
  transition: all 0s ease-in-out 0s;
}
.paginatoin-area ul.pagination-box li.np-icon a {
  color: var(--font-color-body);
  font-size: 14px;
  height: 30px;
  width: 30px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #F9F9F9;
}
.paginatoin-area ul.pagination-box li.np-icon:hover a {
  color: var(--font-color-header);
}
.paginatoin-area ul.pagination-box li a i {
  display: block;
  {%- if settings.enable_rtl -%}
  -webkit-transform: scaleX(-1);
  -o-transform: scaleX(-1);
  transform: scaleX(-1);
  {%- endif -%}
  line-height: 0;
}
.paginatoin-area ul.pagination-box li a i::before {
  font-weight: 600!important;
}

/*=====================================
  timer css
======================================*/
.single-product-wrap .timer-section{
  position: absolute;
  bottom: 0;
  right: 50%;
  transform: translateX(50%);
  -webkit-transition: all .3s ease-in-out 0s;
  -o-transition: all .3s ease-in-out 0s;
  transition: all .3s ease-in-out 0s;
}
.single-product-wrap .single-product-wrap:hover .timer-section{
