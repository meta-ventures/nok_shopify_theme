/*=====================================
  product label css
======================================*/
.single-product-wrap.stock-out{
  cursor: not-allowed;
}
.single-product-wrap.stock-out .product-image.stock-out{
  position: relative;
}
.single-product-wrap .product-image.stock-out::after{
  content: "";
  position: absolute;
  top: 0;
  {% if settings.enable_rtl %}
  right: 0;
  {% else %}
  left: 0;
  {% endif %}
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
  opacity: 0.5;
  cursor: not-allowed;
}
.deal-section .product-content .product-label span,
.single-product-wrap .product-label span{
  font-size: {{ settings.label_font_size }}px;
  position: absolute;
  top: {{ settings.label_top_pos }}px;
  width: {{ settings.label_width }}px;
  height: {{ settings.label_height }}px;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  {% if settings.label_text_transform == 'capitalize' %}
  text-transform: capitalize;
  {% elsif settings.label_text_transform == 'lowercase' %}
  text-transform: lowercase;
  {% elsif settings.label_text_transform == 'uppercase' %}
  text-transform: uppercase;
  {% else %}
  text-transform: none;
  {% endif %}
  font-weight: {{ settings.label_font_weight }};
  border-radius : {{ settings.label_border_radius }}px;
  line-height: 1;
}
.single-product-wrap .product-label span.new-sale-title{
  color: {{ settings.new_sale_label_color }};
  background-color: {{ settings.new_sale_label_bgcolor }};
}
.deal-section .product-content .product-label span.percent-count,
.single-product-wrap .product-label span.percent-count{
  color: {{ settings.discount_label_color }};
  background-color: {{ settings.discount_label_bgcolor }};
}
.single-product-wrap .product-label span.soldout-title{
  color: {{ settings.solout_label_color }};
  background-color: {{ settings.solout_label_bgcolor }};
  z-index: 1;
}
.single-product-wrap .product-image .product-label span.new-sale-title{
  {% if settings.enable_rtl %}
  right: 10px;
  {% else %}
  left: 10px;
  {% endif %}
}
.single-product-wrap:hover .product-image .product-label span.new-sale-title{
  {% if settings.enable_rtl %}
  transform: translateX(calc(100% + 10px));
  {% else %}
  transform: translateX(calc(-100% - 10px));
  {% endif %}
}
.single-product-wrap .product-image .product-label span.soldout-title{
  {% if settings.enable_rtl %}
  right: 10px;
  {% else %}
  left: 10px;
  {% endif %}
}
.deal-section .product-content .product-label span.percent-count,
.single-product-wrap .product-label span.percent-count{
  {% if settings.enable_rtl %}
  left: 15px;
  {% else %}
  right: 15px;
  {% endif %}
}
.deal-section .product-content:hover .product-label span.percent-count,
.single-product-wrap:hover .product-image .product-label span.percent-count{
  opacity: 0;
  visibility: visible;
}
.single-product-wrap .product-image .product-label span.new-sale-title,
.single-product-wrap .product-image .product-label span.percent-count,
.single-product-wrap:hover .product-image .product-label span.new-title,
.single-product-wrap:hover .product-image .product-label span.sale-title,
.deal-section .product-content .product-label span.percent-count,
.single-product-wrap:hover .product-image .product-label span.percent-count{
  -webkit-transition: all 0.3s ease-in-out 0s;
  -o-transition: all 0.3s ease-in-out 0s;
  transition: all 0.3s ease-in-out 0s;
}

/*=====================================
  product quantity css
======================================*/
.product-quantity .cart-plus-minus{
  display: flex;
  align-items: center;
  position: relative;
  border: 1px solid #e2e2e2;
